{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"custom": false,
			"customGradient": false,
			"customDuotone": false,
			"duotone": [
				{
					"colors": ["#5a6eaa", "#d6dbea"],
					"slug": "blue-lightblue",
					"name": "Blue LightBlue"
				},
				{
					"colors": ["#8fb48e", "#e0eddf"],
					"slug": "green-lightgreen",
					"name": "Green LightGreen"
				},
				{
					"colors": ["#a98bb1", "#EADEED"],
					"slug": "lavender-lightlavender",
					"name": "Lavender LightLavender"
				},
				{
					"colors": ["#d9c195", "#f5ebda"],
					"slug": "gold-lightgold",
					"name": "Gold LightGold"
				},
				{
					"colors": ["#b4a798", "#e3dacf"],
					"slug": "taupe-lighttaupe",
					"name": "Taupe LightTaupe"
				},
				{
					"colors": ["#B2A89A", "#DED8CE"],
					"slug": "primary-lightprimary",
					"name": "Primary LightPrimary"
				}
			],
			"palette": [
				{
					"slug": "base",
					"color": "#1e1e1e",
					"name": "Base"
				},

				{ "slug": "blue-lighter", "color": "#e6ebf3", "name": "BlueLighter" },
				{ "slug": "blue-light", "color": "#d2dbeb", "name": "BlueLight" },
				{ "slug": "blue", "color": "#C0D2FC", "name": "Blue" },
				{ "slug": "blue-er", "color": "#5a6eaa", "name": "Blue-er" },
				{ "slug": "blue-est", "color": "#4b5582", "name": "Blue-est" },
				{ "slug": "blue-dark", "color": "#3f4769", "name": "BlueDark" },
				{ "slug": "blue-darker", "color": "#2a2e41", "name": "BlueDarker" },

				{ "slug": "green-lighter", "color": "#e2ebe0", "name": "greenLighter" },
				{ "slug": "green-light", "color": "#c4d8c2", "name": "greenLight" },
				{ "slug": "green", "color": "#8fb48e", "name": "Green" },
				{ "slug": "green-er", "color": "#4d7c4f", "name": "Green-er" },
				{ "slug": "green-est", "color": "#2e4e31", "name": "Green-est" },
				{ "slug": "green-dark", "color": "#144113", "name": "GreenDark" },
				{ "slug": "green-darker", "color": "#111d12", "name": "GreenDarker" },

				{
					"slug": "lavender-lighter",
					"color": "#f4f1f6",
					"name": "LavenderLighter"
				},
				{ "slug": "lavender-light", "color": "#ddd1e1", "name": "LavenderLight" },
				{ "slug": "lavender", "color": "#a98bb1", "name": "Lavender" },
				{ "slug": "lavender-er", "color": "#84658b", "name": "Lavender-er" },
				{ "slug": "lavender-est", "color": "#5E3769", "name": "Lavender-est" },
				{ "slug": "lavender-dark", "color": "#431a4f", "name": "LavenderDark" },
				{ "slug": "lavender-darker", "color": "#371542", "name": "LavenderDarker" },

				{ "slug": "gold-lighter", "color": "#f4eee0", "name": "GoldLighter" },
				{ "slug": "gold-light", "color": "#e8dac0", "name": "GoldLight" },
				{ "slug": "gold", "color": "#d9c195", "name": "Gold" },
				{ "slug": "gold-er", "color": "#bf8d50", "name": "Gold-er" },
				{ "slug": "gold-est", "color": "#93613b", "name": "Gold-est" },
				{ "slug": "gold-dark", "color": "#61412d", "name": "GoldDark" },
				{ "slug": "gold-darker", "color": "#342116", "name": "GoldDarker" },

				{ "slug": "taupe-lighter", "color": "#edebe7", "name": "TaupeLighter" },
				{ "slug": "taupe-light", "color": "#DBD5CD", "name": "TaupeLight" },
				{ "slug": "taupe", "color": "#b4a798", "name": "Taupe" },
				{ "slug": "taupe-er", "color": "#8d7869", "name": "Taupe-er" },
				{ "slug": "taupe-est", "color": "#766458", "name": "Taupe-est" },
				{ "slug": "taupe-dark", "color": "#50453e", "name": "TaupeDark" },
				{ "slug": "taupe-darker", "color": "#2a2320", "name": "TaupeDarker" },

				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "primaryLight",
					"color": "#DED8CE",
					"name": "primaryLight"
				},
				{
					"slug": "primary",
					"color": "#B2A89A",
					"name": "primary"
				},
				{
					"slug": "primaryDark",
					"color": "#58524B",
					"name": "primaryDark"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			]
		},
		"layout": {
			"contentSize": "1080px",
			"wideSize": "1200px"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"units": ["rem", "vh", "vw", "%"]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"customFontSize": false,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "Oldenburg",
					"fontFace": [
						{
							"fontFamily": "Oldenburg",
							"fontStyle": "normal",
							"src": ["file:./assets/fonts/oldenburg-v22-latin-regular.woff2"]
						}
					]
				},
				{
					"name": "Sans Serif",
					"slug": "sans-serif",
					"fontFamily": "Quicksand",
					"fontFace": [
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/quicksand-v31-latin-300.woff2"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/quicksand-v31-latin-regular.woff2"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": ["file:./assets/fonts/quicksand-v31-latin-500.woff2"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": ["file:./assets/fonts/quicksand-v31-latin-600.woff2"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/quicksand-v31-latin-700.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "1rem",
					"name": "S",
					"fluid": false
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "M",
					"fluid": false
				},
				{
					"slug": "large",
					"size": "1.75rem",
					"name": "L",
					"fluid": {
						"min": "1.5rem",
						"max": "1.75rem"
					}
				},
				{
					"slug": "x-large",
					"size": "2rem",
					"name": "XL",
					"fluid": {
						"min": "1.75rem",
						"max": "2rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "2.25rem",
					"name": "XX:",
					"fluid": {
						"min": "2.25rem",
						"max": "2.75rem"
					}
				},
				{
					"slug": "xxx-large",
					"size": "3rem",
					"name": "XX:",
					"fluid": {
						"min": "2.75rem",
						"max": "3rem"
					}
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/navigation": {
				"css": "& .wp-block-site-title{ margin:0; font-weight:400;}",
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-content": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--60)",
						"bottom": "var(--wp--preset--spacing--60)"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/post-template": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)",
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/query-pagination": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/search": {
				"css": ".wp-block-search__button-inside .wp-block-search__inside-wrapper{border: none;}",
				"typography": {
					"lineHeight": "1"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--base)"
		},
		"elements": {
			"button": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)"
					}
				},
				"border": {
					"color": "var(--wp--preset--color--transparent)",
					"width": "4px",
					"style": "solid",
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			}
		},
		"spacing": {
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No title",
			"postTypes": ["post", "page"]
		}
	]
}

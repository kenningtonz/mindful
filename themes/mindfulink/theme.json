{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"custom": false,
			"customGradient": false,
			"customDuotone": false,
			"palette": [
				{
					"slug": "base",
					"color": "#1e1e1e",
					"name": "Base"
				},
				{
					"slug": "blueDark",
					"color": "#283d50",
					"name": "Blue"
				},
				{
					"slug": "blue",
					"color": "#5a6eaa",
					"name": "Blue"
				},
				{
					"slug": "blueLight",
					"color": "#d6dbea",
					"name": "BlueLight"
				},
				{
					"slug": "greenDark",
					"color": "#144113",
					"name": "GreenDark"
				},
				{
					"slug": "green",
					"color": "#8fb48e",
					"name": "Green"
				},
				{
					"slug": "greenLight",
					"color": "#e0eddf",
					"name": "GreenLight"
				},
				{
					"slug": "lavenderDark",
					"color": "#431a4f",
					"name": "LavenderDark"
				},
				{
					"slug": "lavender",
					"color": "#a98bb1",
					"name": "Lavender"
				},
				{
					"slug": "lavenderLight",
					"color": "#EADEED",
					"name": "LavenderLight"
				},
				{
					"slug": "goldDark",
					"color": "#49391d",
					"name": "GoldDark"
				},
				{
					"slug": "gold",
					"color": "#d9c195",
					"name": "Gold"
				},
				{
					"slug": "goldLight",
					"color": "#f5ebda",
					"name": "GoldLight"
				},
				{
					"slug": "taupeDark",
					"color": "#6e491e",
					"name": "TaupeDark"
				},
				{
					"slug": "taupe",
					"color": "#b4a798",
					"name": "Taupe"
				},
				{
					"slug": "taupeLight",
					"color": "#e3dacf",
					"name": "TaupeLight"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			]
		},
		"layout": {
			"contentSize": "1080px",
			"wideSize": "1440px"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"units": [
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"customFontSize": false,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "Oldenburg",
					"fontFace": [
						{
							"fontFamily": "Oldenburg",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/oldenburg-v22-latin-regular.woff2"
							]
						}
					]
				},
				{
					"name": "Sans Serif",
					"slug": "sans-serif",
					"fontFamily": "Quicksand",
					"fontFace": [
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/quicksand-v31-latin-300.woff2"
							]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/quicksand-v31-latin-regular.woff2"
							]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/quicksand-v31-latin-500.woff2"
							]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/quicksand-v31-latin-600.woff2"
							]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/quicksand-v31-latin-700.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "1rem",
					"name": "S",
					"fluid": false
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "M",
					"fluid": false
				},
				{
					"slug": "large",
					"size": "1.75rem",
					"name": "L",
					"fluid": {
						"min": "1.5rem",
						"max": "1.75rem"
					}
				},
				{
					"slug": "extra-large",
					"size": "2.25rem",
					"name": "XL",
					"fluid": {
						"min": "2rem",
						"max": "2.25rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "3rem",
					"name": "XX:",
					"fluid": {
						"min": "2.25rem",
						"max": "3rem"
					}
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/navigation": {
				"css": "& .wp-block-site-title{ margin:0; font-weight:400;}",
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-content": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--60)",
						"bottom": "var(--wp--preset--spacing--60)"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/post-template": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)",
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/query-pagination": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/search": {
				"css": ".wp-block-search__button-inside .wp-block-search__inside-wrapper{border: none;}",
				"typography": {
					"lineHeight": "1"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--base)"
		},
		"elements": {
			"button": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)"
					}
				},
				"border": {
					"color": "var(--wp--preset--color--transparent)",
					"width": "4px",
					"style": "solid",
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			}
		},
		"spacing": {
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No title",
			"postTypes": [
				"post",
				"page"
			]
		}
	]
}